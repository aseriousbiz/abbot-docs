{
  "metadata": [
    {
      "src": [
        {
          "src": "../_build",
          "files": [
            "Abbot.Scripting.Interfaces.dll"
          ]
        }
      ],
      "dest": "reference/csharp",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "netcoreapp3.1"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "reference/csharp/**.yml",
          "reference/csharp/**.md"
        ]
      },
      {
        "files": [
          "reference/python/**.yml",
          "reference/python/**.md"
        ]
      },
      {
        "files": [
          "reference/javascript/**.yml",
          "reference/javascript/**.md"
        ]
      },
      {
        "files": [
          "reference/*.md",
          "reference/toc.yml"
        ]
      },
      {
        "files": [
          "guides/**.md",
          "guides/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "templates/abbot/**.css",
          "templates/abbot/**.js",
          "templates/abbot/**.svg",
          "templates/abbot/**.ico",
          "templates/abbot/**.png",
          "CNAME"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "dest": "../_site",
    "globalMetadata": {
      "_appTitle": "Abbot Docs",
      "_appFooter": "Copyright Â© A Serious Business, Inc.",
      "_enableSearch": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/abbot"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "repositoryRoot": "../"
  }
}
